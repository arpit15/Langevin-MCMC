<scene version="2.1.0">

<!-- Defaults, these can be set via the command line: -Darg=value -->

	<default name="spp" value="128"/>
	<default name="resx" value="324"/>
	<default name="resy" value="243"/>

<!-- Camera and Rendering Parameters -->

	<integrator type="path">
		<integer name="max_depth" value="12"/>
	</integrator>
	<sensor type="perspective" id="Camera">
		<string name="fov_axis" value="x"/>
		<float name="fov" value="102.000003"/>
		<float name="near_clip" value="0.100000"/>
		<float name="far_clip" value="1000.000000"/>
		<transform name="to_world">
			<rotate x="1" angle="179.999991348578"/>
			<rotate y="1" angle="-4.785268921648044e-21"/>
			<rotate z="1" angle="180.00000500895632"/>
			<translate value="0.000000 0.020000 0.000000"/>
		</transform>
		<sampler type="independent">
			<integer name="sample_count" value="$spp"/>
		</sampler>
		<film type="hdrfilm">
			<integer name="width" value="$resx"/>
			<integer name="height" value="$resy"/>
			<boolean name="banner" value="false"/>
		</film>
	</sensor>

<!-- Materials -->

	<bsdf type="twosided" id="default-bsdf">
		<bsdf type="diffuse"/>
	</bsdf>
	<bsdf type="diffuse" id="mat-Material">
		<rgb value="1.000000 0.000000 0.300000" name="reflectance"/>
	</bsdf>

